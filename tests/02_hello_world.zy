println -> usize:
  &[]u8 text
{
  sz1 := write(text);
  sz2 := write(b"\n");

  sz1 + sz2
};

main -> i32:
  i32 argc,
  &[]str argv
{
  println(b"Hello, world!");

  0i32
};
