type str := []u32;
type c_str := []u8;

println -> usize:
  &str text
{
  sz1 := write(text);
  sz2 := write(b"\n");

  sz1 + sz2
};

main -> i32:
  i32 argc,
  &[]c_str argv
{
  println(b"Hello, world!");

  0
};
